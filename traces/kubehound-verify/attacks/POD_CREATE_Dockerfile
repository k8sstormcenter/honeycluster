FROM ubuntu:24.04
RUN apt-get update && apt-get install -y curl apt-transport-https ca-certificates  gnupg strace 
RUN /bin/sh -c curl -LO "https://dl.k8s.io/release/v1.32.0/bin/linux/amd64/kubectl" && chmod +x kubectl && mv kubectl /usr/local/bin/
ENTRYPOINT ["/bin/bash" "-c"] 


